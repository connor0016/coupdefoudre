{
    "name": "coupdefoudre_node",
    "version": "1.0.0",
    "description": "\"sass compile\"",
    "main": "index.js",
    "config": {
        "cssDir": "dist/assets/css",
        "scssDir": "src/sass"
    },
    "scripts": {
        "clean/css": "rimraf dist/assets/css && mkdirp dist/assets/css",
        "css/sass": "node-sass src/sass/style.scss -o dist/assets/css --output-style expanded",
        "pro/css/sass": "node-sass src/sass/style.scss -o dist/assets/css --output-style compressed",
        "css/postcss": "postcss dist/assets/css/style.css --use autoprefixer -o dist/assets/css/style.css -c=postcss.config.js",
        "dev/css": "npm run clean/css && npm-run-all -p css/*",
        "pro/css": "npm run clean/css && npm run pro/css/sass && npm run css/postcss && npm run css/watch",
        "clean/js": "rimraf dist/assets/js && mkdirp dist/assets/js",
        "js/build": "webpack --mode-development --watch",
        "js": "npm run clean/js && npm-run-all -p js/*",
        "clean/img": "rimraf dist/assets/images && mkdirp dist/assets/images",
        "img/imagemin": "node imagemin.js"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "gulp": "^4.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.6.0",
        "@babel/preset-env": "^7.6.0",
        "autoprefixer": "^9.6.1",
        "babel-core": "^6.26.3",
        "babel-preset-es2015": "^6.24.1",
        "babel-register": "^6.26.0",
        "browser-sync": "^2.26.7",
        "cssnano": "^4.1.10",
        "del": "^5.1.0",
        "glob": "^7.1.4",
        "gulp-babel": "^8.0.0",
        "gulp-clean-css": "^4.2.0",
        "gulp-debug": "^4.0.0",
        "gulp-imagemin": "^6.1.0",
        "gulp-notify": "^3.2.0",
        "gulp-plumber": "^1.2.1",
        "gulp-postcss": "^8.0.0",
        "gulp-rename": "^1.4.0",
        "gulp-sass": "^4.0.2",
        "gulp-uglify": "^3.0.2",
        "imagemin": "^7.0.0",
        "imagemin-gifsicle": "^6.0.1",
        "imagemin-keep-folder": "^5.3.2",
        "imagemin-mozjpeg": "^8.0.0",
        "imagemin-pngquant": "^8.0.0",
        "imagemin-svgo": "^7.0.0",
        "node-sass": "^4.12.0",
        "nodemon": "^1.19.2",
        "npm-run-all": "^4.1.5",
        "onchange": "^6.1.0",
        "postcss": "^7.0.18",
        "postcss-cli": "^6.1.3",
        "rollup": "^1.21.4",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "ts-loader": "^6.1.0",
        "typescript": "^3.6.3",
        "uglify-js": "^3.6.0",
        "watch": "^1.0.2",
        "webpack": "^4.40.2",
        "webpack-cli": "^3.3.9",
        "webpack-dev-server": "^3.8.1",
        "webpack-stream": "^5.2.1"
    }
}
